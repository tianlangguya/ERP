<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.mxp.erp.dao.RolePermissionDao">

	<select id="getByRoleId"
		resultType="com.mxp.erp.entity.RolePermissionEntity">
		select * from sys_role_permission where ROLE_ID =
		#{roleId}
	</select>

</mapper>